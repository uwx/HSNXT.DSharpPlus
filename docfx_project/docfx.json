{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "docfx_project/**"
          ]
        }
      ],
      "dest": "api/HSNXT.DSharpPlus",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    },
    {
      "src": [
        {
          "src": "../../DSharpPlus",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "docs/**",

            "DSharpPlus.CommandsNext/**",
            "DSharpPlus.Interactivity/**",
            "DSharpPlus.WebSocket.**"

          ]
        }
      ],
      "dest": "api/DSharpPlus",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "api/**/toc.yml",
          "api/**/index.md",
          "api/toc.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/toc.yml",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "fileMetadata":{
    },
    "globalMetadata" : {
      "_appFooter": "© 2017-2019 uwx. DSharpPlus is copyright © 2017-2019 DSharpPlus development team. uwx's DSharpPlus Extensions is not supported or endorsed by the DSharpPlus development team.",
      "_enableSearch": "true"
    },
    "template": [
      "dsplus_template"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}