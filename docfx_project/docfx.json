{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "**.cs"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "**/doxygen_out/**",
            "_site/**"
          ]
        },
        {
          "src": "../../DSharpPlus",
          "files": [
            "**.cs"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "**/docs/**",
            "**/DSharpPlus.CommandsNext/**"
          ]
        }
      ],
      "dest": "api",
      "filter": "filter_config.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}